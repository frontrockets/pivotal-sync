{
  "version": 2,
  "name": "connect-github-pivotal",
  "alias": "connect-github-pivotal.now.sh",
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [{ "src": "/", "dest": "/index.js" }],
  "env": {
    "APP_ID": "@app-id",
    "WEBHOOK_SECRET": "@webhook-secret",
    "PRIVATE_KEY": "@private-key",
    "PIVOTAL_TOKEN": "@pivotal-token"
  }
}
